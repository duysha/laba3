#include <iostream>

using namespace std;

double SearchX(double a1, double b1, double a2, double b2) {

    return ((b2 - b1) / (a1 - a2));
}

double SearchY(double x, double a1, double b1) {

    return (a1 * x + b1);
}
void Print(double x, double y) {

    cout << x << '\t' << y;
}
int main() {

    double a1, b1, a2, b2;
    cin >> a1 >> b1 >> a2 >> b2;

    if (a1 == a2) {
        cout << "прямые ||";
        return 0;
    }
    double x = SearchX(a1, b1, a2, b2);
    double y = SearchY(x, a1, b1);
    Print(x, y);

    return 0;
}
